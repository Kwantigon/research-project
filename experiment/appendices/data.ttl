@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix tc: <https://slovník.gov.cz/datový/turistické-cíle/pojem/> .
@prefix bf: <https://slovník.gov.cz/generický/bezbariérové-přístupy/pojem/> .
@prefix qty: <https://slovník.gov.cz/generický/množství/pojem/> .

#################################################################
# Tourist Destination 1
#################################################################

<http://example.org/destination/castle-A>
    a tc:turistický-cíl ;
    rdfs:label "Castle A"@en ;
    tc:kapacita [
        a qty:množství ;
        qty:hodnota "150"^^xsd:integer
    ] ;
    tc:bezbariérovost [
        a bf:bezbariérový-přístup ;
        bf:výtah-u-hlavního-vstupu "true" ;
        bf:výtah-v-interiéru "true"
    ] ;
    tc:kouření-povoleno "false" .

